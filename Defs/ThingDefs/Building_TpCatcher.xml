<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- <ThingDef ParentName="BuildingBase"> -->
	<ThingDef ParentName="FurnitureWithQualityBase">
    <defName>LTF_TpCatcher</defName>
    <label>catcher</label>
    <description>A wooden structure serving as a teleport vector. Must be associated with a Teleport spot.</description>
    <!-- <thingClass>Building_WorkTable</thingClass> -->
	<thingClass>Building_Storage</thingClass>
    <graphicData>
		<texPath>Things/Building/TpCatcher/TpCatcher</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<shaderType>CutoutComplex</shaderType>
		<drawSize>(1,1)</drawSize>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
	<randomizeRotationOnSpawn>true</randomizeRotationOnSpawn>
	
	<statBases>
		<WorkToBuild>1000</WorkToBuild>
		<MaxHitPoints>20</MaxHitPoints>
		<Flammability>1.5</Flammability>
		<MarketValue>100</MarketValue>
		<EntertainmentStrengthFactor>1</EntertainmentStrengthFactor>
	</statBases>
	
	<resourcesFractionWhenDeconstructed>.2</resourcesFractionWhenDeconstructed>
	<tradeTags>
		<li>Furniture</li>
    </tradeTags>
	<minifiedDef>MinifiedFurniture</minifiedDef>
	<tickerType>Normal</tickerType>
	
	<scatterableOnMapGen>false</scatterableOnMapGen>
	<useHitPoints>True</useHitPoints>
    <size>(1,1)</size>
	
	<!-- <clearBuildingArea>false</clearBuildingArea> -->
	<rotatable>false</rotatable>
	<stealable>true</stealable>
    <defaultPlacingRot>North</defaultPlacingRot>
	
    <designationCategory>Security</designationCategory>
    <passability>Standable</passability>
    <hasInteractionCell>False</hasInteractionCell>
    <!-- <interactionCellOffset>(0,0,-1)</interactionCellOffset> -->
	
    <surfaceType>Item</surfaceType>

    <building>
		<sowTag>SupportPlantsOnly</sowTag>
		<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
		
		<ai_chillDestination>true</ai_chillDestination>
		<isEdifice>false</isEdifice>
		<preventDeteriorationOnTop>false</preventDeteriorationOnTop>
		
		<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
		<isInert>true</isInert>
		<defaultStorageSettings>
			<priority>Normal</priority>
			<filter>
				<categories>
					<li>Weapons</li>
				</categories>
			</filter>
		</defaultStorageSettings>
	</building>
		
		<inspectorTabs><li>ITab_Storage</li></inspectorTabs>
		<!-- <li>ITab_Bills</li> -->
	
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>50</costStuffCount>
		<costList>
			<WoodLog>40</WoodLog>
			<Steel>5</Steel>
		</costList>

    <comps>
		<li Class="CompProperties_Explosive">
			<explosiveRadius>1</explosiveRadius>
			<explosiveDamageType>EMP</explosiveDamageType>
			<startWickHitPointsPercent>0.05</startWickHitPointsPercent>
			<wickTicks>
				<min>15</min>
				<max>70</max>
			</wickTicks>
		</li>
		
		<li Class="CompProperties_Breakdownable"/>
		<li><compClass>CompQuality</compClass></li>
	
		<li Class="LTF_Teleport.CompProperties_LTF_TpSpot">
			<weightBase>20</weightBase>
			<weightSpectrum>80</weightSpectrum>
			<cooldownBase>7200</cooldownBase>
			<cooldownSpectrum>-3600</cooldownSpectrum>
			<PowerRequired>false</PowerRequired>
		</li>
	
		<li Class="CompProperties_Glower">
			<glowRadius>1</glowRadius>
			<!-- pink -->
			<glowColor>(138,43,226,0)</glowColor>
		</li>

		<!-- Allows to target something -->
		<li Class="CompProperties_Targetable">
			<compClass>LTF_Teleport.CompTargetable_Building</compClass>
		</li>

		<!-- Associates object to effects -->
		<li Class="CompProperties_Usable">
			<useJob>UseItem</useJob>
			<useLabel>Link</useLabel>
		</li>
		<!-- target effect -->
		<li>
			<compClass>LTF_Teleport.CompTargetEffect_TpSpotLink</compClass>
		</li>

    </comps>
	<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	<researchPrerequisites>
		<li>LTF_Research_TpSpot</li>
	</researchPrerequisites>
  </ThingDef>

</Defs>