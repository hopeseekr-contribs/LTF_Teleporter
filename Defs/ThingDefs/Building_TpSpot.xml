<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- <ThingDef ParentName="BuildingBase"> -->
	<ThingDef ParentName="FurnitureWithQualityBase">
    <defName>LTF_TpSpot</defName>
    <label>Teleport spot</label>
    <description>A place to the infinite and beyond.
Requires a teleport bench.</description>
    <!-- <thingClass>Building_WorkTable</thingClass> -->
	<thingClass>Building_Storage</thingClass>
    <graphicData>
		<texPath>Things/Building/TpSpot/TpSpot</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<shaderType>CutoutComplex</shaderType>
		<drawSize>(1,1)</drawSize>
    </graphicData>
    <altitudeLayer>FloorEmplacement</altitudeLayer>
	
	<statBases>
		<WorkToBuild>3200</WorkToBuild>
		<MaxHitPoints>100</MaxHitPoints>
		<Flammability>1.0</Flammability>
		<MarketValue>1200</MarketValue>
		<EntertainmentStrengthFactor>1</EntertainmentStrengthFactor>
	</statBases>
	
	<resourcesFractionWhenDeconstructed>.3</resourcesFractionWhenDeconstructed>
	<tradeTags>
		<li>Furniture</li>
    </tradeTags>
	<minifiedDef>MinifiedFurniture</minifiedDef>
	<tickerType>Normal</tickerType>
	
	<scatterableOnMapGen>false</scatterableOnMapGen>
	<useHitPoints>True</useHitPoints>
    <size>(1,1)</size>
	
	<!-- <clearBuildingArea>false</clearBuildingArea> -->
	<rotatable>True</rotatable>
    <defaultPlacingRot>North</defaultPlacingRot>
	
    <designationCategory>Security</designationCategory>
    <passability>Standable</passability>
    <hasInteractionCell>False</hasInteractionCell>
    <!-- <interactionCellOffset>(0,0,-1)</interactionCellOffset> -->
	
    <surfaceType>Item</surfaceType>

    <building>
		<sowTag>SupportPlantsOnly</sowTag>
		<canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
		
		<ai_chillDestination>false</ai_chillDestination>
		<isEdifice>false</isEdifice>
		
		<preventDeteriorationOnTop>false</preventDeteriorationOnTop>
		<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
		
		<defaultStorageSettings>
			<priority>Important</priority>
			<filter>
				<categories>
					<li>Weapons</li>
				</categories>
			</filter>
		</defaultStorageSettings>
	</building>
	
	<inspectorTabs>
		<li>ITab_Storage</li>
	</inspectorTabs>
			<!-- <li>ITab_Bills</li> -->
	
		    <stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		
		<costStuffCount>150</costStuffCount>
		<costList>
			<Steel>50</Steel>
			<Uranium>5</Uranium>
			<Component>2</Component>
		</costList>
	
    <comps>
	<li Class="CompProperties_Power">
		<compClass>CompPowerTrader</compClass>
		<basePowerConsumption>120</basePowerConsumption>
		<startElectricalFires>true</startElectricalFires>
	</li>
	
	<li Class="CompProperties_Flickable"/>
	
	<!-- <li><compClass>CompQuality</compClass></li> -->
	
	<li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
			<li>LTF_TpBench</li>
        </linkableFacilities>
	</li>
    
	<li Class="LTF_Teleport.CompProperties_LTF_TpSpot">
		<weightBase>50</weightBase>
		<weightSpectrum>160</weightSpectrum>
		<cooldownBase>3600</cooldownBase>
		<cooldownSpectrum>-2400</cooldownSpectrum>
	</li>
	
    </comps>

	<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	
	<placeWorkers>
		<li>PlaceWorker_ShowFacilitiesConnections</li>
	</placeWorkers>
	
		<researchPrerequisites>
			<li>LTF_Research_TpSpot</li>
		</researchPrerequisites>
  </ThingDef>

</Defs>